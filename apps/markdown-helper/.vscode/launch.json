{
  "version": "2.0.0",
  "configurations": [{
    "name": "Run Extension",
    "type": "extensionHost",
    "request": "launch",
    "args": [
      "--extensionDevelopmentPath=${workspaceFolder}"
    ],
    "outFiles": [
      "${workspaceFolder}/dist/**/*.js",
      "!**/node_modules/**"
    ],
    "preLaunchTask": "npm: watch"
  }]
}